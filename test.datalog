ancestor : Relation(Int, Int, Int).
ancestor(X, Y, K) :- parent(X, Y).
ancestor(X, Y, K) :- parent(X, Z), ancestor(Z, Y, 0).

path(X, X).
path(X, Y) :- neighbor(X, Y).
path(X, Y) :- path(X, Z), path(Z, Y).

